public class Main { 
public static void main(String args[]) {   
String str = "abbcccdda";    
int k =str.length()-1;   
String g=str;   
for(int i=0;i<str.length()-1;i++){
if(str.charAt(i)==str.charAt(i+1)){
str=str.substring(0,i) + str.substring(i + 1);
i--;    
}  
}  
System.out.println(str);
}
}
